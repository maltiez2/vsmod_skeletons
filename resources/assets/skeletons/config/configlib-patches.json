{
  "version": 0,
  "patches": {
    "other": {
      "skeletons:config/customplayermodels/skeleton.json": {
        "skeleton/ExtraTraits": "(remove_models_traits) ? empty_array : value",
        "skeleton/AvailableClasses": "(remove_models_traits) ? empty_array : value",
        "skeleton/SkipClasses": "(remove_models_traits) ? skip_classes : value"
      }
    },
    "boolean": {
      "skeletons:patches/model-traits.json": {
        "0/enabled": "not(remove_models_traits)"
      }
    }
  },
  "constants": {
    "empty_array": [],
    "skip_classes": ["skeletons-commoner", "skeletons-archer", "skeletons-crossbowman", "skeletons-spearman", "skeletons-swordsman", "skeletons-smith"]
  },
  "settings": [
    {
      "code": "remove_models_traits",
      "type": "boolean",
      "ingui": "skeletons:setting-remove_models_traits",
      "default": false
    }
  ]
}